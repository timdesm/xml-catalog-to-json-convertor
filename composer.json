{
    "name": "tim/xml-catalog-to-json-convertor",
    "authors": [],
    "require": {
        "php": ">7.0",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
